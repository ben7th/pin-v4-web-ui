<% content_cell do %>
  <% mplist @rss_feeds ,:for=>RssFeed %>
  <%= will_paginate @rss_feeds,{:position=>:paper} %>
<% end %>

<% content_for :javascripts do %>
  <%= javascript_include_tag 'views/rss_feed' %>
<% end %>
<% content_for :head do %>
  <%= stylesheet_link_tag "/stylesheets/views/rss" %>
<% end %>