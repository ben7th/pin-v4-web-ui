<object height="360" width="440" type="application/x-shockwave-flash" data="<%= video_src %>" style="visibility: visible;">
  <param name="quality" value="high" />
  <param name="allowScriptAccess" value="always" />
  <param name="wmode" value="transparent" />
  <param name="allowFullscreen" value="true" />
  <param name="flashvars" value="playMovie=true" />
  <param name="movie" value="<%= video_src %>" />
</object>
<!-- 各种浏览器都兼容的插入Flash元素代码 !>
<!-- param movie 用来兼容IE !>